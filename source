AimbotExecute.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
AimbotExecute.BackgroundTransparency = 1.000
AimbotExecute.Position = UDim2.new(0.0349462368, 0, 0.626666665, 0)
AimbotExecute.Size = UDim2.new(0, 74, 0, 20)
AimbotExecute.Image = "rbxassetid://2790382281"
